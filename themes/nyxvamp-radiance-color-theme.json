{
  "name": "NyxVamp - Radiance",
  "type": "light",
  "colors": {
    // UI Elements
    "activityBar.background": "#E8E8F0",
    "activityBar.foreground": "#1E1E2E",
    "activityBarBadge.background": "#9655FF",
    "activityBarBadge.foreground": "#F7F7FF",

    "sideBar.background": "#E8E8F0",
    "sideBar.foreground": "#1E1E2E",
    "sideBarSectionHeader.background": "#E8E8F0",
    "sideBarSectionHeader.foreground": "#1E1E2E",

    "statusBar.background": "#E8E8F0",
    "statusBar.foreground": "#1E1E2E",
    "statusBar.noFolderBackground": "#F7F7FF",
    "statusBar.debuggingBackground": "#E78284",
    "statusBar.debuggingForeground": "#1E1E2E",

    "titleBar.activeBackground": "#F7F7FF",
    "titleBar.activeForeground": "#1E1E2E",
    "titleBar.inactiveBackground": "#F7F7FF",
    "titleBar.inactiveForeground": "#5A5570",

    "editor.background": "#F7F7FF",
    "editor.foreground": "#1E1E2E",
    "editor.selectionBackground": "#E8D5FF",
    "editor.lineHighlightBackground": "#E8E8F0",
    "editorCursor.foreground": "#9655FF",
    "editorWhitespace.foreground": "#5A5570",

    "editorLineNumber.foreground": "#5A5570",
    "editorLineNumber.activeForeground": "#1E1E2E",

    "tab.activeBackground": "#E8E8F0",
    "tab.activeForeground": "#1E1E2E",
    "tab.inactiveBackground": "#F7F7FF",
    "tab.inactiveForeground": "#5A5570",
    "tab.border": "#E8E8F0",

    "button.background": "#9655FF",
    "button.foreground": "#F7F7FF",
    "button.hoverBackground": "#6B46C1",
    "button.secondaryBackground": "#5A5570",
    "button.secondaryForeground": "#F7F7FF",
    "button.secondaryHoverBackground": "#4A4560",

    "dropdown.background": "#E8E8F0",
    "dropdown.foreground": "#1E1E2E",
    "dropdown.border": "#5A5570",

    "input.background": "#E8E8F0",
    "input.foreground": "#1E1E2E",
    "input.border": "#5A5570",
    "input.placeholderForeground": "#5A5570",

    "badge.background": "#9655FF",
    "badge.foreground": "#F7F7FF",

    "editorGroupHeader.tabsBackground": "#E8E8F0",
    "editorGroupHeader.tabsBorder": "#E8E8F0",
    "editorGroup.emptyBackground": "#F7F7FF"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5A5570",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "scope": ["string.special"],
      "settings": {
        "foreground": "#9F1239"
      }
    },
    {
      "scope": ["constant", "constant.builtin"],
      "settings": {
        "foreground": "#9F1239",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["number"],
      "settings": {
        "foreground": "#C2410C"
      }
    },
    {
      "scope": ["boolean"],
      "settings": {
        "foreground": "#9F1239"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#005F87",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#9655FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["operator"],
      "settings": {
        "foreground": "#4A4560"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#1E1E2E"
      }
    },
    {
      "scope": ["variable.builtin"],
      "settings": {
        "foreground": "#9F1239",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["type", "entity.name.type"],
      "settings": {
        "foreground": "#6B46C1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["attribute", "entity.name.attribute"],
      "settings": {
        "foreground": "#9655FF"
      }
    },
    {
      "scope": ["namespace"],
      "settings": {
        "foreground": "#9655FF"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#1E1E2E"
      }
    },
    {
      "scope": ["symbol"],
      "settings": {
        "foreground": "#9F1239",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#9655FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#1E1E2E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#1E1E2E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.link.url"],
      "settings": {
        "foreground": "#005F87",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.link.text"],
      "settings": {
        "foreground": "#9655FF"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#5A5570",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.diagnostic.error"],
      "settings": {
        "foreground": "#E78284",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diagnostic.warning"],
      "settings": {
        "foreground": "#E5C890",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diagnostic.info"],
      "settings": {
        "foreground": "#8CAAEE",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diagnostic.hint"],
      "settings": {
        "foreground": "#8BD5CA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["diff.add"],
      "settings": {
        "foreground": "#A6DA95"
      }
    },
    {
      "scope": ["diff.delete"],
      "settings": {
        "foreground": "#E78284"
      }
    },
    {
      "scope": ["diff.change"],
      "settings": {
        "foreground": "#E5C890"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#1E1E2E",
    "variable.readonly": "#9F1239",
    "variable.defaultLibrary": "#9F1239",
    "variable.parameter": "#9F1239",
    "function": "#005F87",
    "function.constructor": "#005F87",
    "function.static": "#005F87",
    "keyword": "#9655FF",
    "keyword.control": "#9655FF",
    "type": "#6B46C1",
    "type.builtin": "#6B46C1",
    "constant": "#9F1239",
    "constant.builtin": "#9F1239",
    "string": "#B8860B",
    "number": "#C2410C",
    "boolean": "#9F1239",
    "comment": "#5A5570",
    "operator": "#4A4560",
    "punctuation": "#1E1E2E",
    "namespace": "#9655FF",
    "attribute": "#9655FF",
    "symbol": "#9F1239",
    "markup.heading": "#9655FF",
    "markup.bold": "#1E1E2E",
    "markup.italic": "#1E1E2E",
    "markup.link.url": "#005F87",
    "markup.link.text": "#9655FF",
    "markup.quote": "#5A5570"
  },
  "semanticHighlighting": true
}