{
  "name": "NyxVamp - Radiance",
  "type": "light",
  "colors": {
    // UI Elements
    "activityBar.background": "#E8E8F0",                  // background
    "activityBar.foreground": "#1E1E2E",                  // foreground
    "activityBarBadge.background": "#9655FF",             // badge background
    "activityBarBadge.foreground": "#F7F7FF",             // badge foreground

    "sideBar.background": "#E8E8F0",                      // menu_bg
    "sideBar.foreground": "#1E1E2E",                      // menu_fg
    "sideBarSectionHeader.background": "#E8E8F0",         // menu_bg
    "sideBarSectionHeader.foreground": "#1E1E2E",         // menu_fg

    "statusBar.background": "#E8E8F0",                    // status_bg
    "statusBar.foreground": "#1E1E2E",                    // status_fg
    "statusBar.noFolderBackground": "#E8E8F0",            // status_inactive_bg
    "statusBar.debuggingBackground": "#D78284",          // status during debugging
    "statusBar.debuggingForeground": "#1E1E2E",          // status_debugging_fg (assuming similar to status_fg)

    "titleBar.activeBackground": "#E8E8F0",               // background
    "titleBar.activeForeground": "#1E1E2E",               // foreground
    "titleBar.inactiveBackground": "#E8E8F0",             // background
    "titleBar.inactiveForeground": "#6E6A86",             // status_inactive_fg

    "editor.background": "#F7F7FF",                       // background
    "editor.foreground": "#1E1E2E",                       // foreground
    "editor.selectionBackground": "#FEF5BC",              // selection
    "editor.lineHighlightBackground": "#E8E8F0",          // cursorline
    "editorCursor.foreground": "#F7F7FF",                 // cursor_fg
    "editorWhitespace.foreground": "#5E5A76",             // whitespace

    "editorLineNumber.foreground": "#6E6A86",             // line_nr
    "editorLineNumber.activeForeground": "#1E1E2E",       // line_nr_selected

    "tab.activeBackground": "#C0C0CE",                    // background
    "tab.activeForeground": "#1E1E20",                    // background
    "tab.inactiveBackground": "#E8E8F0",                  // menu_bg
    "tab.inactiveForeground": "#6E6A86",                  // status_inactive_fg
    "tab.border": "#E8E8F0",                              // menu_bg

    "button.background": "#9655FF",                       // button.background
    "button.foreground": "#F7F7FF",                       // button.foreground
    "button.hoverBackground": "#C0C0CE",                  // button.hoverBackground
    "button.secondaryBackground": "#5E5A76",              // button.secondaryBackground
    "button.secondaryForeground": "#C0C0CE",              // button.secondaryForeground
    "button.secondaryHoverBackground": "#2E2E30",         // button.secondaryHoverBackground

    "dropdown.background": "#E8E8F0",                     // dropdown_bg
    "dropdown.foreground": "#C0C0CE",                     // dropdown_fg
    "dropdown.border": "#5E5A76",                         // dropdown_border

    "input.background": "#E8E8F0",                        // input_bg
    "input.foreground": "#C0C0CE",                        // input_fg
    "input.border": "#5E5A76",                            // input_border
    "input.placeholderForeground": "#5E5A76",              // input_placeholder_fg

    "badge.background": "#9655FF",                        // badge.background
    "badge.foreground": "#F7F7FF",                        // badge.foreground

    "editorGroupHeader.tabsBackground": "#E8E8F0",         // menu_bg
    "editorGroupHeader.tabsBorder": "#E8E8F0",             // menu_bg
    "editorGroup.emptyBackground": "#E8E8F0"              // background
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6E6A86",                          // comment_fg
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#FEF5BC"                           // string_fg
      }
    },
    {
      "scope": ["string.special"],
      "settings": {
        "foreground": "#F28FAD"                           // string_special_fg
      }
    },
    {
      "scope": ["constant", "constant.builtin"],
      "settings": {
        "foreground": "#F28FAD",                          // constant_fg
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["number"],
      "settings": {
        "foreground": "#D8A080"                           // number_fg
      }
    },
    {
      "scope": ["boolean"],
      "settings": {
        "foreground": "#F28FAD"                           // boolean_fg
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#005F87",                          // function_fg
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#9655FF",                          // keyword_fg
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["operator"],
      "settings": {
        "foreground": "#6E6A86"                           // operator_fg
      }
    },
    {
      "scope": ["variable", "variable.builtin"],
      "settings": {
        "foreground": "#1E1E2E",                          // variable_fg
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["type", "entity.name.type"],
      "settings": {
        "foreground": "#A0A0D0",                          // type_fg
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["attribute", "entity.name.attribute"],
      "settings": {
        "foreground": "#9655FF"                           // attribute_fg
      }
    },
    {
      "scope": ["namespace"],
      "settings": {
        "foreground": "#9655FF"                           // namespace_fg
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#1E1E2E"                           // punctuation_fg
      }
    },
    {
      "scope": ["symbol"],
      "settings": {
        "foreground": "#F28FAD",                          // symbol_fg
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#9655FF",                          // markup_heading_fg
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#1E1E2E",                          // markup_bold_fg
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#1E1E2E",                          // markup_italic_fg
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.link.url"],
      "settings": {
        "foreground": "#005F87",                          // markup_link_url_fg
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.link.text"],
      "settings": {
        "foreground": "#9655FF"                           // markup_link_text_fg
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#6E6A86",                          // markup_quote_fg
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.diagnostic.error"],
      "settings": {
        "foreground": "#E78284",                          // error_fg
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diagnostic.warning"],
      "settings": {
        "foreground": "#E5B880",                          // warning_fg
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diagnostic.info"],
      "settings": {
        "foreground": "#7A9CCC",                          // info_fg
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diagnostic.hint"],
      "settings": {
        "foreground": "#7BB5A8",                          // hint_fg
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["diff.add"],
      "settings": {
        "foreground": "#86BA75"                           // diff_add_fg
      }
    },
    {
      "scope": ["diff.delete"],
      "settings": {
        "foreground": "#D78284"                           // diff_delete_fg
      }
    },
    {
      "scope": ["diff.change"],
      "settings": {
        "foreground": "#D5B880"                           // diff_change_fg
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#1E1E2E",                                // variable
    "variable.readonly": "#F28FAD",                       // variable.readonly
    "variable.defaultLibrary": "#F28FAD",
    "variable.parameter": "#F28FAD",
    "function": "#005F87",                                 // function
    "function.constructor": "#005F87",
    "function.static": "#005F87",
    "keyword": "#9655FF",                                  // keyword
    "keyword.control": "#9655FF",
    "type": "#A0A0D0",                                     // type
    "type.builtin": "#A0A0D0",
    "constant": "#F28FAD",                                 // constant
    "constant.builtin": "#F28FAD",
    "string": "#FEF5BC",                                   // string
    "number": "#D8A080",                                   // number
    "boolean": "#F28FAD",                                  // boolean
    "comment": "#6E6A86",                                  // comment
    "operator": "#6E6A86",                                 // operator
    "punctuation": "#1E1E2E",                              // punctuation
    "namespace": "#9655FF",                                // namespace
    "attribute": "#9655FF",                                 // attribute
    "symbol": "#F28FAD",                                    // symbol
    "markup.heading": "#9655FF",                           // markup.heading
    "markup.bold": "#1E1E2E",                              // markup.bold
    "markup.italic": "#1E1E2E",                            // markup.italic
    "markup.link.url": "#005F87",                          // markup.link.url
    "markup.link.text": "#9655FF",                         // markup.link.text
    "markup.quote": "#6E6A86"                              // markup.quote
  },
  "semanticHighlighting": true
}