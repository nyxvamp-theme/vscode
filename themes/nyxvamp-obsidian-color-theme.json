{
  "name": "NyxVamp - Obsidian",
  "type": "dark",
  "colors": {
    // UI Elements
    "activityBar.background": "#000A0F",
    "activityBar.foreground": "#C0C0CE",
    "activityBarBadge.background": "#F28FAD",
    "activityBarBadge.foreground": "#000A0F",

    "sideBar.background": "#000A0F",
    "sideBar.foreground": "#C0C0CE",
    "sideBarSectionHeader.background": "#1E1E20",
    "sideBarSectionHeader.foreground": "#C0C0CE",

    "statusBar.background": "#1E1E20",
    "statusBar.foreground": "#C0C0CE",
    "statusBar.noFolderBackground": "#000A0F",
    "statusBar.debuggingBackground": "#D78284",
    "statusBar.debuggingForeground": "#C0C0CE",

    "titleBar.activeBackground": "#000A0F",
    "titleBar.activeForeground": "#C0C0CE",
    "titleBar.inactiveBackground": "#000A0F",
    "titleBar.inactiveForeground": "#5E5A76",

    "editor.background": "#000A0F",
    "editor.foreground": "#C0C0CE",
    "editor.selectionBackground": "#2E2E30",
    "editor.lineHighlightBackground": "#1E1E20",
    "editorCursor.foreground": "#F28FAD",
    "editorWhitespace.foreground": "#5E5A76",

    "editorLineNumber.foreground": "#5E5A76",
    "editorLineNumber.activeForeground": "#C0C0CE",

    "tab.activeBackground": "#1E1E20",
    "tab.activeForeground": "#C0C0CE",
    "tab.inactiveBackground": "#000A0F",
    "tab.inactiveForeground": "#5E5A76",
    "tab.border": "#000A0F",

    "button.background": "#F28FAD",
    "button.foreground": "#000A0F",
    "button.hoverBackground": "#F5C2E7",
    "button.secondaryBackground": "#5E5A76",
    "button.secondaryForeground": "#C0C0CE",
    "button.secondaryHoverBackground": "#2E2E30",

    "dropdown.background": "#000A0F",
    "dropdown.foreground": "#C0C0CE",
    "dropdown.border": "#5E5A76",

    "input.background": "#000A0F",
    "input.foreground": "#C0C0CE",
    "input.border": "#5E5A76",
    "input.placeholderForeground": "#5E5A76",

    "badge.background": "#F28FAD",
    "badge.foreground": "#000A0F",

    "editorGroupHeader.tabsBackground": "#000A0F",
    "editorGroupHeader.tabsBorder": "#000A0F",
    "editorGroup.emptyBackground": "#000A0F"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5E5A76",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#8FBF8F"
      }
    },
    {
      "scope": ["string.special"],
      "settings": {
        "foreground": "#F28FAD"
      }
    },
    {
      "scope": ["constant", "constant.builtin"],
      "settings": {
        "foreground": "#F28FAD",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["number"],
      "settings": {
        "foreground": "#D8A080"
      }
    },
    {
      "scope": ["boolean"],
      "settings": {
        "foreground": "#F28FAD"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#7FAFD7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#F5C2E7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["operator"],
      "settings": {
        "foreground": "#C0C0CE"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#C0C0CE"
      }
    },
    {
      "scope": ["variable.builtin"],
      "settings": {
        "foreground": "#F28FAD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["type", "entity.name.type"],
      "settings": {
        "foreground": "#A0A0D0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["attribute", "entity.name.attribute"],
      "settings": {
        "foreground": "#F5C2E7"
      }
    },
    {
      "scope": ["namespace"],
      "settings": {
        "foreground": "#A090C0"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#C0C0CE"
      }
    },
    {
      "scope": ["symbol"],
      "settings": {
        "foreground": "#F28FAD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#F5C2E7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#F5C2E7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#F5C2E7",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.link.url"],
      "settings": {
        "foreground": "#7FAFD7",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.link.text"],
      "settings": {
        "foreground": "#F5C2E7"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#5E5A76",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.diagnostic.error"],
      "settings": {
        "foreground": "#D78284",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diagnostic.warning"],
      "settings": {
        "foreground": "#D5B880",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diagnostic.info"],
      "settings": {
        "foreground": "#7A9CCC",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diagnostic.hint"],
      "settings": {
        "foreground": "#7BB5A8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["diff.add"],
      "settings": {
        "foreground": "#86BA75"
      }
    },
    {
      "scope": ["diff.delete"],
      "settings": {
        "foreground": "#D78284"
      }
    },
    {
      "scope": ["diff.change"],
      "settings": {
        "foreground": "#D5B880"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#C0C0CE",
    "variable.readonly": "#F28FAD",
    "variable.defaultLibrary": "#F28FAD",
    "variable.parameter": "#F28FAD",
    "function": "#7FAFD7",
    "function.constructor": "#7FAFD7",
    "function.static": "#7FAFD7",
    "keyword": "#F5C2E7",
    "keyword.control": "#F5C2E7",
    "type": "#A0A0D0",
    "type.builtin": "#A0A0D0",
    "constant": "#F28FAD",
    "constant.builtin": "#F28FAD",
    "string": "#8FBF8F",
    "number": "#D8A080",
    "boolean": "#F28FAD",
    "comment": "#5E5A76",
    "operator": "#C0C0CE",
    "punctuation": "#C0C0CE",
    "namespace": "#A090C0",
    "attribute": "#F5C2E7",
    "symbol": "#F28FAD",
    "markup.heading": "#F5C2E7",
    "markup.bold": "#F5C2E7",
    "markup.italic": "#F5C2E7",
    "markup.link.url": "#7FAFD7",
    "markup.link.text": "#F5C2E7",
    "markup.quote": "#5E5A76"
  },
  "semanticHighlighting": true
}